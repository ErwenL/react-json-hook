{
  "name": "react-json-hook",
  "description": "A customizable headless React hook component for creating JSON viewers.",
  "version": "0.5.0",
  "author": "Erwen Li <erwenlovelife@outlook.com>",
  "main": ".dist/react-json-hook.umd.js",
  "module": "./dist/react-json-hook.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/react-json-hook.es.js",
      "require": "./dist/react-json-hook.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo-config.ts",
    "predeploy": "vite build --config vite.gh-pages-config.ts",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --outDir demos/dist"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ErwenL/react-json-hook.git"
  },
  "homepage": "https://erwenl.github.io/react-json-hook",
  "keywords": [
    "react",
    "react-hook",
    "hook",
    "component",
    "interactive",
    "json",
    "viewer",
    "json-viewer",
    "json-display",
    "json-tree"
  ]
}
